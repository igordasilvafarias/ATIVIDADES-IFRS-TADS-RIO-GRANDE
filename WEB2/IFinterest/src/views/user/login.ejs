<div class="container">
    <div class="row">
		<div class="col-md-4 col-md-offset-4">
    		<div class="panel panel-default">
			  	<div class="panel-heading">
			    	<h3 class="panel-title">LOGIN</h3>
			 	</div>
			  	<div class="panel-body">
					<form accept-charset="UTF-8" role="form" method="POST">
						<fieldset>
							<div class="form-group">
								<label for="inputName" class="mt-2">Name</label>
								<input class="form-control" name="name" type="text" id="inputName" required autofocus>
									<% if (locals.userError) { %>
										<div class="alert alert-danger" role="alert">
											Usuário <%= locals.name %> não encontrado.
										</div>
									<% } %>
							</div>
							<div class="form-group">
								<label for="inputPassword" class="mt-2">Password</label>
								<input class="form-control" name="password" type="password" id="inputPassword" required>
								<% if (locals.passwordError) { %>
									<div class="alert alert-danger" role="alert">
										<%= locals.passwordError %>
									</div>
								<% } %>
							</div>
							</div>
							<input class="btn btn-lg btn-success btn-block" type="submit" value="Login">
						</fieldset>
					</form>
			    </div>
			</div>
		</div>
	</div>
</div>

