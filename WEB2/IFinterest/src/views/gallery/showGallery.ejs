    <!-- HEAD -->
    <%- include('../components/head.ejs') %>
    <!-- HEAD -->

    <body>

    	<!-- NAV -->
    	<%- include('../components/nav.ejs') %>
    	<!-- NAV -->

    	<!-- Photos Grid-->

    	<% if (gallerys.length == 0) { %>
    	<div class="container">
    		<div class="row">
    			<div class="col-md-4 col-md-offset-4">
    				<div class="panel panel-default">
    					<div class="panel-heading">
    						<h3 class="panel-title">Galerias</h3>
    					</div>
    					<div class="panel-body">
    						<fieldset>
    							<div class="form-group">
    								<h3 class="panel-title"> Não foram encontrado Galerias Adicionadas!</h3>
    							</div>
    							<div class="form-group">
    								<input class="btn btn-lg btn-success btn-block" onclick="location.href='/profile'"
    									value="Voltar">
    							</div>
    						</fieldset>
    					</div>
    				</div>
    			</div>
    		</div>
    	</div>
    	<% } else { %>
			<div class="container">
				<div class="row">
					<div class="col-md-4 col-md-offset-4">
						<div class="panel panel-default">
							<div class="panel-heading">
								<h3 class="panel-title">Galerias</h3>
							</div>
							<div class="panel-body">
								<fieldset>
									<table class="table" >
										<thead>
											<th>Identificatição</th>
											<th>Deletar</th>
										</thead>
										<tbody>
											<% for (let i = 0; i < gallerys.length; i++) { %>
												<tr>
													<td> <a href="/gallery/<%= gallerys[i].id %>"> <h3 class="panel-title"> <%= gallerys[i].identification %></h3> </a></td>
													<td> <a href="/gallery/delete/<%= gallerys[i].id %>"> <h3 class="panel-title"> deletar </h3> </a></td>
												</tr>
											<% } %>
										</tbody>
									</table>
									<div class="form-group">
										<input class="btn btn-lg btn-success btn-block" onclick="location.href='/profile'"
											value="Voltar">
									</div>
								</fieldset>
							</div>
						</div>
					</div>
				</div>
			</div>
    	<% } %>

    	<!-- Photos Grid-->

    	<!-- Footer -->
    	<%- include('../components/footer.ejs') %>
    	<!-- Footer -->

    	</div>

    </body>

    </html>